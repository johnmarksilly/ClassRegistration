            <script src="~/app/components/umnCourse/umnCourse.controller.js"></script>
            <script src="~/app/components/umnCourse/umnCourse.directive.js"></script>
            <link rel="stylesheet" href="~/app/components/umnCourse/umnCourse.style.css" />

