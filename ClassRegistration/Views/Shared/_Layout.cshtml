            <script src="~/app/components/umnSimpleCourse/umnSimpleCourse.controller.js"></script>
            <script src="~/app/components/umnSimpleCourse/umnSimpleCourse.directive.js"></script>
            <link rel="stylesheet" href="~/app/components/umnSimpleCourse/umnSimpleCourse.style.css" />

