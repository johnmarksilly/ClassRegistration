            <script src="~/app/components/umnShoppingCart/umnShoppingCart.controller.js"></script>    
            <script src="~/app/components/umnShoppingCart/umnShoppingCart.directive.js"></script>
            <link rel="stylesheet" href="~/app/components/umnShoppingCart/umnShoppingCart.style.css" />

